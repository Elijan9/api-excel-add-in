<Window x:Class="OctopartApi.ProxyAuthPrompt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Proxy Authentication Required" Height="212.001" Width="370.667" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid>
        <Label Content="Username:" HorizontalAlignment="Left" Margin="30,63,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="30,94,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textboxUsername" HorizontalAlignment="Left" Height="23" Margin="101,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="217"/>
        <PasswordBox x:Name="textboxPassword" HorizontalAlignment="Left" Margin="101,97,0,0" VerticalAlignment="Top" Width="217" Height="23"/>
        <TextBlock x:Name="textMessage" HorizontalAlignment="Left" Height="48" Margin="10,10,0,0" TextWrapping="Wrap" Text="The server {0} requires a username and password" VerticalAlignment="Top" Width="345"/>
        <Button x:Name="buttonOK" Content="OK" Margin="0,0,99,10" Height="21" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="84" IsDefault="True" Click="buttonOK_Click"/>
        <Button x:Name="buttonCancel" Content="Cancel" Margin="0,0,10,10" Height="21" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="84" IsCancel="True" Click="buttonCancel_Click"/>
    </Grid>
</Window>
